    "browser-test": "node --harmony node_modules/.bin/rc-tools run browser-test",
    "browser-test-cover": "node --harmony node_modules/.bin/rc-tools run browser-test-cover"
  },
  "devDependencies": {
    "font-awesome": "~4.2.0",
    "expect.js": "~0.3.1",
    "precommit-hook": "^1.0.7",
    "rc-server": "3.x",
    "rc-tools": "4.x",
    "react": "~0.13.0"
  },
  "dependencies": {
    "dom-scroll-into-view": "1.x",
    "rc-util": "2.x"
